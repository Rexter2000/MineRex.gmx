{
    ds_grid_set(chunk, i, j, 1);
    instance_create(i * 16, j * 16, oGrass);
}

// 129 - 256 = randome dirt or stone
for (i = 0; i &lt; width; i += 1)
{
    for (j = 129; j &lt; height; j += 1) // clvl below - values set
    {
    blk = irandom(2)+2; // using only 2 and 3 for randoms
    if (blk = 2)
        {
        ds_grid_set(chunk, i, j, blk);
        instance_create(i * 16, j * 16, oDirt);
        }
        else if (blk = 3)
        {
        ds_grid_set(chunk, i, j, blk-1);
        instance_create(i * 16, j * 16, oDirt);
        }
        else if (blk = 4)
        {
        ds_grid_set(chunk, i, j, blk-1);
        instance_create(i * 16, j * 16, oStone);
        }
    }       
}

// create Guy1
