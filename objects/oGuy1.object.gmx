if keyboard_check_pressed(vk_left)
{
    x -= 16;
    sprite_index = sGuy1L;
}
if keyboard_check_pressed(vk_right)
{
    item = ds_grid_get(oCPU.chunk, oGuy1.x/16 + 1, oGuy1.y/16 + 1);
    item1 = ds_grid_get(oCPU.chunk, oGuy1.x/16 + 1, oGuy1.y/16);
    
//    show_message(item);
    
    if (item == 0) 
        x += 16;
    else if (item &gt; 0 &amp;&amp; item1 == 0)
    {
        x += 16; 
        y -= 16;
    }
        
    sprite_index = sGuy1R;
}
if keyboard_check_pressed(vk_up)
{
    item = ds_grid_get(oCPU.chunk, oGuy1.x/16, oGuy1.y/16 + 1);
    if item &lt;&gt; 0 then y -= 16;
}
if keyboard_check_pressed(vk_down)
{
    y += 0;
    item = ds_grid_get(oCPU.chunk, oGuy1.x/16, oGuy1.y/16 + 2);
    ds_grid_set(oCPU.chunk, oGuy1.x/16, oGuy1.y/16 + 2, 0);
    if item = 1 then
        with(instance_position(oGuy1.x,oGuy1.y+32,oGrass)) instance_destroy();
    else if item = 2 then
        with(instance_position(oGuy1.x,oGuy1.y+32,oDirt)) instance_destroy();
    else if item = 3 then
        with(instance_position(oGuy1.x,oGuy1.y+32,oStone)) instance_destroy();
     y += 16; 
}
if keyboard_check_pressed(vk_space)
{
    y -= 16;
}  
if keyboard_check(vk_nokey)
{
    sprite_index = sGuy1S;
}

// 16x16 snap
with (oDirt)
    {
    if !place_snapped(16, 16)
       {
       move_snap(16, 16);
       }
    }

